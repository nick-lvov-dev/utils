{
  "name": "@lvovno/utils",
  "version": "0.11.1",
  "description": "Custom hooks, helpers & types",
  "author": "nick-lvov-dev",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick-lvov-dev/utils.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c && tsc --declaration --emitDeclarationOnly --declarationDir .",
    "start": "rollup -c -w",
    "test": "jest",
    "peers": "install-self-peers -- --ignore-scripts"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^2.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@team-griffin/install-self-peers": "^1.1.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "deepmerge": "^4.2.2",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.1.0",
    "react-test-renderer": "^16.13.1",
    "rollup": "^1.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "files": [
    "./**/*.js",
    "./**/*.d.ts"
  ],
  "keywords": [
    "react",
    "react hooks",
    "typescript",
    "npm"
  ],
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/nick-lvov-dev/utils/issues"
  },
  "homepage": "https://github.com/nick-lvov-dev/utils#readme"
}
